<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

<display-name>Archetype Created Web Application</display-name>


  
  <servlet>
  	<servlet-name>FindAllController</servlet-name>
  	<display-name>FindAllController</display-name>
  	<description></description>
  	<servlet-class>com.createiq.controller.FindAllController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteProductController</servlet-name>
  	<display-name>DeleteProductController</display-name>
  	<description></description>
  	<servlet-class>com.createiq.controller.DeleteProductController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateController</servlet-name>
  	<display-name>UpdateController</display-name>
  	<description></description>
  	<servlet-class>com.createiq.controller.UpdateController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddingController</servlet-name>
  	<display-name>AddingController</display-name>
  	<description></description>
  	<servlet-class>com.createiq.controller.AddingController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddProductController</servlet-name>
  	<display-name>AddProductController</display-name>
  	<description></description>
  	<servlet-class>com.createiq.controller.AddProductController</servlet-class>
  </servlet>
 
  <servlet-mapping>
  	<servlet-name>FindAllController</servlet-name>
  	<url-pattern>/findController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteProductController</servlet-name>
  	<url-pattern>/deleteById</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateController</servlet-name>
  	<url-pattern>/updateById</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddingController</servlet-name>
  	<url-pattern>/adding</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddProductController</servlet-name>
  	<url-pattern>/addProduct</url-pattern>
  </servlet-mapping>

  
<welcome-file-list><welcome-file>index.jsp</welcome-file></welcome-file-list>
</web-app>
